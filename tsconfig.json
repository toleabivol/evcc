{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "assets/js/**/*",
        "assets/js/src/**/*.vue"
    ],
    "compilerOptions": {
        "composite": true,
        "noImplicitAny": false,
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    }
}